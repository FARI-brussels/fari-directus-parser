{
  "name": "fari-directus-parser",
  "version": "1.0.3",
  "description": "A parser for Directus data.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FARI-brussels/fari-directus-parser.git"
  },
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.cjs"
    }
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "format": "npx prettier . --check",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --write .",
    "format:full": "eslint . --fix && prettier --write ."
  },
  "keywords": [
    "directus",
    "parser"
  ],
  "author": "Mosa",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/jest": "^29.5.14",
    "eslint": "^9.13.0",
    "globals": "^15.11.0",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.12.2"
  },
  "dependencies": {
    "@directus/sdk": "^18.0.0"
  }
}
